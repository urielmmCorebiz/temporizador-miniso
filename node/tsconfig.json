{
	"compilerOptions": {
		"types": ["node", "xml2js", "jsontoxml"],
		"typeRoots": ["node_modules/@types"],
		"target": "es2017",
		"alwaysStrict": true,
		"module": "commonjs",
		"sourceMap": false,
		"outDir": "service",
		"allowJs": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitThis": true
	},
	"typeAcquisition": {
		"enable": false
	},
	"include": ["typings.d.ts", "./**/*"],
	"exclude": ["node_modules"]
}
